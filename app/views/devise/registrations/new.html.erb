  <section id="content" class="m-t-lg wrapper-md animated fadeInDown">
     <a class="nav-brand">Salon Objectives</a>
    <div class="row m-n">
      <div class="col-md-4 col-md-offset-4 m-t-lg">
        <section class="panel">
          <header class="panel-heading bg bg-primary text-center">
            Sign up
          </header>
<%= simple_form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => {:class => 'panel-body' }) do |f| %>
<div class="form-group">
  <%= f.input :first_name, :autofocus => true %></div>
  <div class="form-group">
  <%= f.input :last_name %></div>
  <div class="form-group">
  <%= f.input :email, :required => true %></div>
  <div class="form-group">
  <%= f.input :password, :required => true %></div>
   <div class="form-group">
  <%= f.input :password_confirmation, :required => true %></div>

  <%= f.button :submit, 'Sign up', :class => 'btn btn-info' %>
<% end %>
<div class="line line-dashed"></div>
  <p class="text-muted text-center"><small>Already have an account?</small></p>
<%= render "devise/shared/links" %>
       </form>
        </section>
      </div>
    </div>
  </section>